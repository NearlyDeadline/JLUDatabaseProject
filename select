select * from course;

select title from course;

select distinct title from course natural join section;
select distinct title from course where course_id in (select course_id from section);

select * from student limit 6;

select S.ID, S.name from student as S where not exists ((select course_id from course where dept_name = 'Comp. Sci.') except (select T.course_id from takes as T where S.ID = T.ID)); //不支持except，待修改

select distinct course_id from section where semester = 'Spring' and year = '2009' and course_id in (select course_id from section where course_id not in (select course_id from section where year = '2010'));